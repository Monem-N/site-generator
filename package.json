{
  "name": "docsify-site-generator",
  "type": "module",
  "version": "1.0.0",
  "description": "A documentation-driven website generator with Docsify integration",
  "main": "dist/index.js",
  "bin": {
    "site-generator": "dist/cli.js",
    "site-generator-enhanced": "dist/cli-enhanced.js"
  },
  "scripts": {
    "// Build scripts": "",
    "build": "tsc",
    "build:clean": "rm -rf dist && tsc",
    "build:fixed": "tsc --project tsconfig.fixed.json",
    "build:with-types": "./scripts/build-with-types.sh",
    "build:relaxed": "./scripts/build-relaxed.sh",
    "build:super-relaxed": "./scripts/build-super-relaxed.sh",
    "// Development scripts": "",
    "start": "node dist/cli.js",
    "start:enhanced": "node dist/cli-enhanced.js",
    "dev": "tsc --watch",
    "dev:serve": "tsc && node dist/cli-enhanced.js --serve",
    "dev:watch": "tsc && node dist/cli-enhanced.js --watch --serve",
    "// Test scripts": "",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coverage:check": "./scripts/test-coverage.sh",
    "test:fixed": "jest --config=jest.fixed.js",
    "test:safe": "./scripts/run-tests.sh",
    "// Code quality scripts": "",
    "check:types": "./scripts/check-types.sh",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "// Fix scripts": "",
    "fix-and-build": "./scripts/fix-and-build.sh",
    "fix:all": "npm run fix:imports && npm run fix:tests && npm run fix:assertions && npm run fix:variables && npm run fix:paths && npm run fix:decorators && npm run fix:import-styles && npm run fix:navigation && npm run fix:plugin-loader && npm run fix:types && npm run lint:fix",
    "fix:imports": "./scripts/fix-imports.sh",
    "fix:tests": "./scripts/fix-tests.sh",
    "fix:assertions": "./scripts/fix-type-assertions.sh",
    "fix:variables": "./scripts/fix-variable-names.sh",
    "fix:paths": "./scripts/fix-import-paths.sh",
    "fix:decorators": "./scripts/fix-decorators.sh",
    "fix:import-styles": "./scripts/fix-import-styles.sh",
    "fix:navigation": "./scripts/fix-navigation-generator.sh",
    "fix:plugin-loader": "./scripts/fix-plugin-loader.sh",
    "fix:types": "npx ts-migrate add-missing-members .",
    "// Cleanup scripts": "",
    "cleanup": "./scripts/cleanup.sh",
    "cleanup:tsconfig": "./scripts/cleanup-tsconfig.sh",
    "// Lifecycle scripts": "",
    "prepare": "husky install",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "documentation",
    "website",
    "generator",
    "docsify",
    "markdown",
    "react"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-image": "^3.0.3",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@types/handlebars": "^4.0.40",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "express": "^4.18.2",
    "front-matter": "^4.0.2",
    "handlebars": "^4.7.8",
    "marked": "^4.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "workbox-build": "^7.3.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@types/autoprefixer": "^9.7.2",
    "@types/cssnano": "^5.0.0",
    "@types/ejs": "^3.1.5",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/js-yaml": "^4.0.9",
    "@types/marked": "^4.0.8",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "@types/rollup": "^0.51.4",
    "@types/sharp": "^0.31.1",
    "@types/swagger-parser": "^4.0.3",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "babel-jest": "^29.5.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^9.1.7",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.7",
    "swagger-parser": "^10.0.3",
    "ts-jest": "^29.1.0",
    "ts-migrate": "^0.1.35",
    "ts-node": "^10.9.2",
    "ttypescript": "^1.5.15",
    "typescript": "^5.0.3",
    "typescript-transform-paths": "^3.5.5"
  }
}
